import React, { useState } from 'react';
import { Routes, Route } from 'react-router-dom';
import LoginPage from './components/LoginPage';
import KitchenPage from './components/KitchenPage';
import OrderPage from './components/OrderPage';
import UserPage from './components/UserPage';
import 'bootstrap/dist/css/bootstrap.min.css';

function App() {
  const [items, setItems] = useState([
    { id: 1, name: 'Pizza', isAvailable: false, image: 'https://via.placeholder.com/150' },
    { id: 2, name: 'Burger', isAvailable: false, image: 'https://via.placeholder.com/150' },
    { id: 3, name: 'Pasta', isAvailable: false, image: 'https://via.placeholder.com/150' },
  ]);

  const updateItemAvailability = (itemId, isAvailable) => {
    setItems((prevItems) =>
      prevItems.map((item) =>
        item.id === itemId ? { ...item, isAvailable } : item
      )
    );
  };

  const addItem = (newItem) => {
    setItems((prevItems) => [
      ...prevItems,
      { ...newItem, id: prevItems.length + 1 },
    ]);
  };

  return (
    <Routes>
      <Route path="/" element={<LoginPage />} />
      <Route
        path="/kitchen/:kitchenId"
        element={
          <KitchenPage
            items={items}
            updateItemAvailability={updateItemAvailability}
            addItem={addItem}
          />
        }
      />
      <Route path="/orders/:kitchenId" element={<OrderPage items={items} />} />
      <Route path="/user" element={<UserPage items={items} />} />
    </Routes>
  );
}

export default App;
